<DCSubtitle Version="1.0">
	<SubtitleID>836FD175-B8B3-4CF4-8609-C02EC4E1099B</SubtitleID>
	<MovieTitle>Prince_R5</MovieTitle>
	<ReelNumber>5</ReelNumber>
	<Language>fr</Language>
	<LoadFont Id="Arial" URI="arial_princ_loc_fre_r5.ttf" />
	<Font Id="Arial" Color="FFFFFFFF" Effect="border" EffectColor="FF000000"
		Size="42" Weight="normal">
		<Subtitle SpotNumber="1" TimeIn="00:00:10:208" TimeOut="00:00:12:062">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Je t&apos;en prie, Noé.</Text>
		</Subtitle>
		<Subtitle SpotNumber="2" TimeIn="00:00:14:020" TimeOut="00:00:16:083">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">On doit pouvoir faire quelque chose.</Text>
		</Subtitle>
		<Subtitle SpotNumber="3" TimeIn="00:00:17:177" TimeOut="00:00:19:135">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">On peut lancer des cordes.</Text>
		</Subtitle>
		<Subtitle SpotNumber="4" TimeIn="00:00:20:197" TimeOut="00:00:23:031">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Il n&apos;y a pas que des soldats parmi eux.</Text>
		</Subtitle>
		<Subtitle SpotNumber="5" TimeIn="00:00:24:000" TimeOut="00:00:25:239">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Ce sont de pauvres gens.</Text>
		</Subtitle>
		<Subtitle SpotNumber="6" TimeIn="00:00:26:031" TimeOut="00:00:27:135">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Il y a de la place.</Text>
		</Subtitle>
		<Subtitle SpotNumber="7" TimeIn="00:00:27:177" TimeOut="00:00:29:145">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Il n&apos;y a pas de place pour eux.</Text>
		</Subtitle>
		<Subtitle SpotNumber="8" TimeIn="00:01:18:229" TimeOut="00:01:20:187">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Bientôt, tout aura disparu.</Text>
		</Subtitle>
		<Subtitle SpotNumber="9" TimeIn="00:01:23:156" TimeOut="00:01:26:104">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="15.74">Tout ce qui reste de la Création</Text>
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">sera ici.</Text>
		</Subtitle>
		<Subtitle SpotNumber="10" TimeIn="00:01:28:052" TimeOut="00:01:29:020">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Et dehors,</Text>
		</Subtitle>
		<Subtitle SpotNumber="11" TimeIn="00:01:30:239" TimeOut="00:01:33:041">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">à nouveau les eaux du chaos.</Text>
		</Subtitle>
		<Subtitle SpotNumber="12" TimeIn="00:01:35:083" TimeOut="00:01:36:156">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Vous êtes en colère.</Text>
		</Subtitle>
		<Subtitle SpotNumber="13" TimeIn="00:01:37:197" TimeOut="00:01:39:010">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Vous me jugez.</Text>
		</Subtitle>
		<Subtitle SpotNumber="14" TimeIn="00:01:41:229" TimeOut="00:01:43:218">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Je vais vous conter une histoire.</Text>
		</Subtitle>
		<Subtitle SpotNumber="15" TimeIn="00:01:45:062" TimeOut="00:01:49:093">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="15.74">La première que mon père m&apos;a contée,</Text>
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">et que je vous ai contée.</Text>
		</Subtitle>
		<Subtitle SpotNumber="16" TimeIn="00:01:57:218" TimeOut="00:01:59:052">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Au commencement,</Text>
		</Subtitle>
		<Subtitle SpotNumber="17" TimeIn="00:02:00:166" TimeOut="00:02:01:229">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">il n&apos;y avait rien.</Text>
		</Subtitle>
		<Subtitle SpotNumber="18" TimeIn="00:02:04:020" TimeOut="00:02:06:197">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Rien que le silence d&apos;infinies ténèbres.</Text>
		</Subtitle>
		<Subtitle SpotNumber="19" TimeIn="00:02:07:218" TimeOut="00:02:11:187">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="15.74">Mais le souffle du Créateur</Text>
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">murmura dans le vide :</Text>
		</Subtitle>
		<Subtitle SpotNumber="20" TimeIn="00:02:11:229" TimeOut="00:02:13:031">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">&quot;Que la lumière soit !&quot;</Text>
		</Subtitle>
		<Subtitle SpotNumber="21" TimeIn="00:02:14:072" TimeOut="00:02:15:177">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Et la lumière fut.</Text>
		</Subtitle>
		<Subtitle SpotNumber="22" TimeIn="00:02:16:052" TimeOut="00:02:17:156">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Et c&apos;était bien.</Text>
		</Subtitle>
		<Subtitle SpotNumber="23" TimeIn="00:02:18:020" TimeOut="00:02:19:145">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Le premier jour.</Text>
		</Subtitle>
		<Subtitle SpotNumber="24" TimeIn="00:02:20:114" TimeOut="00:02:22:062">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Puis la lumière informe</Text>
		</Subtitle>
		<Subtitle SpotNumber="25" TimeIn="00:02:22:104" TimeOut="00:02:24:239">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="15.74">commença à prendre</Text>
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">de la substance, une forme.</Text>
		</Subtitle>
		<Subtitle SpotNumber="26" TimeIn="00:02:25:083" TimeOut="00:02:26:197">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Le deuxième jour.</Text>
		</Subtitle>
		<Subtitle SpotNumber="27" TimeIn="00:02:27:052" TimeOut="00:02:28:197">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Notre monde était né.</Text>
		</Subtitle>
		<Subtitle SpotNumber="28" TimeIn="00:02:29:135" TimeOut="00:02:31:239">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Notre magnifique et fragile demeure.</Text>
		</Subtitle>
		<Subtitle SpotNumber="29" TimeIn="00:02:32:031" TimeOut="00:02:34:218">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Une lumière forte nourrissait les jours.</Text>
		</Subtitle>
		<Subtitle SpotNumber="30" TimeIn="00:02:36:145" TimeOut="00:02:38:208">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Une lumière faible régnait sur les nuits.</Text>
		</Subtitle>
		<Subtitle SpotNumber="31" TimeIn="00:02:39:041" TimeOut="00:02:40:166">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Il y eut le soir.</Text>
		</Subtitle>
		<Subtitle SpotNumber="32" TimeIn="00:02:41:020" TimeOut="00:02:42:052">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Le matin.</Text>
		</Subtitle>
		<Subtitle SpotNumber="33" TimeIn="00:02:42:229" TimeOut="00:02:44:052">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Un autre jour.</Text>
		</Subtitle>
		<Subtitle SpotNumber="34" TimeIn="00:02:46:239" TimeOut="00:02:49:072">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Les eaux du monde se rassemblèrent,</Text>
		</Subtitle>
		<Subtitle SpotNumber="35" TimeIn="00:02:49:229" TimeOut="00:02:51:135">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">et au milieu d&apos;elles</Text>
		</Subtitle>
		<Subtitle SpotNumber="36" TimeIn="00:02:51:218" TimeOut="00:02:53:145">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">émergea la terre ferme.</Text>
		</Subtitle>
		<Subtitle SpotNumber="37" TimeIn="00:02:54:114" TimeOut="00:02:56:010">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Un autre jour passa.</Text>
		</Subtitle>
		<Subtitle SpotNumber="38" TimeIn="00:02:56:249" TimeOut="00:02:59:041">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Et la terre fit croître des choses.</Text>
		</Subtitle>
		<Subtitle SpotNumber="39" TimeIn="00:02:59:083" TimeOut="00:03:02:156">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="15.74">Une épaisse verdure</Text>
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">recouvrit la Création.</Text>
		</Subtitle>
		<Subtitle SpotNumber="40" TimeIn="00:03:03:156" TimeOut="00:03:06:031">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Les eaux aussi foisonnaient de vie.</Text>
		</Subtitle>
		<Subtitle SpotNumber="41" TimeIn="00:03:07:031" TimeOut="00:03:09:093">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="15.74">Des créatures des abysses</Text>
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">qui ne sont plus.</Text>
		</Subtitle>
		<Subtitle SpotNumber="42" TimeIn="00:03:09:218" TimeOut="00:03:11:166">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Des multitudes de poissons</Text>
		</Subtitle>
		<Subtitle SpotNumber="43" TimeIn="00:03:11:208" TimeOut="00:03:14:166">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="15.74">dont certains parcourent</Text>
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">peut-être encore ces mers.</Text>
		</Subtitle>
		<Subtitle SpotNumber="44" TimeIn="00:03:15:083" TimeOut="00:03:16:156">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Bientôt,</Text>
		</Subtitle>
		<Subtitle SpotNumber="45" TimeIn="00:03:16:197" TimeOut="00:03:18:239">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">le ciel fut sillonné d&apos;oiseaux.</Text>
		</Subtitle>
		<Subtitle SpotNumber="46" TimeIn="00:03:19:104" TimeOut="00:03:20:166">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Il y eut le soir.</Text>
		</Subtitle>
		<Subtitle SpotNumber="47" TimeIn="00:03:20:208" TimeOut="00:03:22:010">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Il y eut le matin.</Text>
		</Subtitle>
		<Subtitle SpotNumber="48" TimeIn="00:03:22:052" TimeOut="00:03:23:124">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Le cinquième jour.</Text>
		</Subtitle>
		<Subtitle SpotNumber="49" TimeIn="00:03:23:166" TimeOut="00:03:26:156">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="15.74">A présent, le monde</Text>
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">grouillait d&apos;êtres vivants.</Text>
		</Subtitle>
		<Subtitle SpotNumber="50" TimeIn="00:03:26:197" TimeOut="00:03:31:083">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="15.74">Tout ce qui rampe, tout ce qui glisse.</Text>
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Toute bête qui foule le sol.</Text>
		</Subtitle>
		<Subtitle SpotNumber="51" TimeIn="00:03:31:124" TimeOut="00:03:33:229">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Et c&apos;était bien. Tout était bien.</Text>
		</Subtitle>
		<Subtitle SpotNumber="52" TimeIn="00:03:34:020" TimeOut="00:03:37:052">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="15.74">Il y avait la lumière, l&apos;air,</Text>
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">l&apos;eau et le sol.</Text>
		</Subtitle>
		<Subtitle SpotNumber="53" TimeIn="00:03:37:093" TimeOut="00:03:38:249">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Propres et inaltérés.</Text>
		</Subtitle>
		<Subtitle SpotNumber="54" TimeIn="00:03:39:041" TimeOut="00:03:41:124">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Plantes, poissons, volatiles et bêtes.</Text>
		</Subtitle>
		<Subtitle SpotNumber="55" TimeIn="00:03:41:208" TimeOut="00:03:44:187">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="15.74">Chacun selon son espèce.</Text>
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Parties du grand Tout.</Text>
		</Subtitle>
		<Subtitle SpotNumber="56" TimeIn="00:03:44:229" TimeOut="00:03:47:177">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="15.74">Chacun à sa place.</Text>
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">L&apos;équilibre régnait.</Text>
		</Subtitle>
		<Subtitle SpotNumber="57" TimeIn="00:03:47:218" TimeOut="00:03:50:249">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="15.74">C&apos;était le paradis.</Text>
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Un joyau dans la main du Créateur.</Text>
		</Subtitle>
		<Subtitle SpotNumber="58" TimeIn="00:03:53:208" TimeOut="00:03:55:124">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Puis Il façonna l&apos;homme.</Text>
		</Subtitle>
		<Subtitle SpotNumber="59" TimeIn="00:03:55:166" TimeOut="00:03:57:083">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Et à son côté, la femme.</Text>
		</Subtitle>
		<Subtitle SpotNumber="60" TimeIn="00:03:57:249" TimeOut="00:03:59:145">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Nos père et mère à tous.</Text>
		</Subtitle>
		<Subtitle SpotNumber="61" TimeIn="00:04:01:031" TimeOut="00:04:02:229">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Il leur donna à choisir.</Text>
		</Subtitle>
		<Subtitle SpotNumber="62" TimeIn="00:04:06:083" TimeOut="00:04:08:083">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Céder à la tentation des ténèbres,</Text>
		</Subtitle>
		<Subtitle SpotNumber="63" TimeIn="00:04:09:104" TimeOut="00:04:12:083">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="15.74">ou rester</Text>
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">dans la bénédiction de la lumière.</Text>
		</Subtitle>
		<Subtitle SpotNumber="64" TimeIn="00:04:22:156" TimeOut="00:04:24:218">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Mais ils mangèrent le fruit défendu.</Text>
		</Subtitle>
		<Subtitle SpotNumber="65" TimeIn="00:04:26:020" TimeOut="00:04:27:218">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Leur innocence disparut.</Text>
		</Subtitle>
		<Subtitle SpotNumber="66" TimeIn="00:04:28:229" TimeOut="00:04:31:177">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="15.74">Et depuis dix générations après Adam,</Text>
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">le péché</Text>
		</Subtitle>
		<Subtitle SpotNumber="67" TimeIn="00:04:32:052" TimeOut="00:04:33:218">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">chemine en nous.</Text>
		</Subtitle>
		<Subtitle SpotNumber="68" TimeIn="00:04:36:145" TimeOut="00:04:37:145">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Frère contre frère.</Text>
		</Subtitle>
		<Subtitle SpotNumber="69" TimeIn="00:04:40:041" TimeOut="00:04:41:239">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Nation contre nation.</Text>
		</Subtitle>
		<Subtitle SpotNumber="70" TimeIn="00:04:42:218" TimeOut="00:04:44:135">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">L&apos;homme contre la Création.</Text>
		</Subtitle>
		<Subtitle SpotNumber="71" TimeIn="00:04:46:187" TimeOut="00:04:48:041">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Nous nous sommes entretués.</Text>
		</Subtitle>
		<Subtitle SpotNumber="72" TimeIn="00:04:50:249" TimeOut="00:04:53:062">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="15.74">Nous avons ravagé le monde.</Text>
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Nous.</Text>
		</Subtitle>
		<Subtitle SpotNumber="73" TimeIn="00:04:53:249" TimeOut="00:04:55:166">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">L&apos;homme a fait cela.</Text>
		</Subtitle>
		<Subtitle SpotNumber="74" TimeIn="00:04:56:135" TimeOut="00:04:57:135">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Tout ce qui était beau.</Text>
		</Subtitle>
		<Subtitle SpotNumber="75" TimeIn="00:04:58:229" TimeOut="00:05:00:135">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Tout ce qui était bien.</Text>
		</Subtitle>
		<Subtitle SpotNumber="76" TimeIn="00:05:01:104" TimeOut="00:05:02:197">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Nous l&apos;avons écrasé.</Text>
		</Subtitle>
		<Subtitle SpotNumber="77" TimeIn="00:05:04:093" TimeOut="00:05:05:124">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">A présent,</Text>
		</Subtitle>
		<Subtitle SpotNumber="78" TimeIn="00:05:06:135" TimeOut="00:05:07:249">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">tout recommence.</Text>
		</Subtitle>
		<Subtitle SpotNumber="79" TimeIn="00:05:09:249" TimeOut="00:05:13:052">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="15.74">Air, eau, terre, plante,</Text>
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">poisson, oiseau et bête.</Text>
		</Subtitle>
		<Subtitle SpotNumber="80" TimeIn="00:05:13:229" TimeOut="00:05:15:177">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Le paradis revient.</Text>
		</Subtitle>
		<Subtitle SpotNumber="81" TimeIn="00:05:16:177" TimeOut="00:05:18:010">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">Mais cette fois,</Text>
		</Subtitle>
		<Subtitle SpotNumber="82" TimeIn="00:05:20:156" TimeOut="00:05:22:041">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
				VPosition="10.00">il n&apos;y aura aucun homme.</Text>
		</Subtitle>
		<Subtitle SpotNumber="83" TimeIn="00:05:23:083" TimeOut="00:05:25:114">
			<Text HAlign="center" HPosition="0.00" VAlign="bottom"
