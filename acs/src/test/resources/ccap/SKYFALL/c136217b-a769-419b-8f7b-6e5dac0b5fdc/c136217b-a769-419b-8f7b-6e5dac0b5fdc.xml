  <SubtitleList>
    <Font Color="FFFFFFFF" Effect="border" EffectColor="FF000000" Size="42" Weight="bold" ID="arial">
      <Subtitle SpotNumber="0001" TimeIn="00:00:04:03" TimeOut="00:00:05:17" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">Was sagt man über so einen Mann?</Text>
      </Subtitle>
      <Subtitle SpotNumber="0002" TimeIn="00:00:05:20" TimeOut="00:00:07:01" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="17.00" Halign="center" Hposition="0.00">JAMES BOND</Text>
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">NACHRUF</Text>
      </Subtitle>
      <Subtitle SpotNumber="0003" TimeIn="00:00:07:12" TimeOut="00:00:09:20" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="17.00" Halign="center" Hposition="0.00">Sie verloren das Laufwerk</Text>
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">vor drei Monaten.</Text>
      </Subtitle>
      <Subtitle SpotNumber="0004" TimeIn="00:00:10:00" TimeOut="00:00:14:19" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="24.00" Halign="center" Hposition="0.00">Es enthielt die Identitäten</Text>
        <Text Valign="bottom" Vposition="17.00" Halign="center" Hposition="0.00">jedes Agenten in terroristischen</Text>
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">Organisationen weltweit.</Text>
      </Subtitle>
      <Subtitle SpotNumber="0005" TimeIn="00:00:14:22" TimeOut="00:00:16:12" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">Ich entschied es.</Text>
      </Subtitle>
      <Subtitle SpotNumber="0006" TimeIn="00:00:17:17" TimeOut="00:00:18:22" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">Die Straße ist zu Ende!</Text>
      </Subtitle>
      <Subtitle SpotNumber="0007" TimeIn="00:00:20:10" TimeOut="00:00:21:15" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">Schießen Sie!</Text>
      </Subtitle>
      <Subtitle SpotNumber="0008" TimeIn="00:00:27:03" TimeOut="00:00:28:05" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">DENK AN DEINE SÜNDEN</Text>
      </Subtitle>
      <Subtitle SpotNumber="0009" TimeIn="00:00:35:05" TimeOut="00:00:37:03" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">007 meldet sich zum Dienst.</Text>
      </Subtitle>
      <Subtitle SpotNumber="0010" TimeIn="00:00:37:08" TimeOut="00:00:38:12" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">Wo waren Sie?</Text>
      </Subtitle>
      <Subtitle SpotNumber="0011" TimeIn="00:00:38:15" TimeOut="00:00:40:03" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">Ich genoss den Tod.</Text>
      </Subtitle>
      <Subtitle SpotNumber="0012" TimeIn="00:00:41:08" TimeOut="00:00:44:05" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="17.00" Halign="center" Hposition="0.00">Ich habe nur eine Frage.</Text>
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">Warum blieben Sie nicht tot?</Text>
      </Subtitle>
      <Subtitle SpotNumber="0013" TimeIn="00:00:44:08" TimeOut="00:00:46:08" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">Zu versagen, ist keine Schande.</Text>
      </Subtitle>
      <Subtitle SpotNumber="0014" TimeIn="00:00:47:03" TimeOut="00:00:49:13" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="17.00" Halign="center" Hposition="0.00">- Ich bin Ihr Quartiermeister.</Text>
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">- Das ist ein Witz.</Text>
      </Subtitle>
      <Subtitle SpotNumber="0015" TimeIn="00:00:49:17" TimeOut="00:00:52:00" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">Walther PPK/S 9mm kurz.</Text>
      </Subtitle>
      <Subtitle SpotNumber="0016" TimeIn="00:00:52:17" TimeOut="00:00:55:08" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="17.00" Halign="center" Hposition="0.00">Sie erkennt Ihre Fingerabdrücke.</Text>
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">Nur Sie können schießen.</Text>
      </Subtitle>
      <Subtitle SpotNumber="0017" TimeIn="00:00:55:12" TimeOut="00:00:58:20" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="24.00" Halign="center" Hposition="0.00">Es ist keine wahllose Waffe,</Text>
        <Text Valign="bottom" Vposition="17.00" Halign="center" Hposition="0.00">sondern eine persönliche</Text>
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">Nachricht.</Text>
      </Subtitle>
      <Subtitle SpotNumber="0018" TimeIn="00:00:59:22" TimeOut="00:01:01:15" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="17.00" Halign="center" Hposition="0.00">- Q.</Text>
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">- 007.</Text>
      </Subtitle>
      <Subtitle SpotNumber="0019" TimeIn="00:01:04:12" TimeOut="00:01:05:22" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">Wo ist Ihr Arbeitgeber?</Text>
      </Subtitle>
      <Subtitle SpotNumber="0020" TimeIn="00:01:06:03" TimeOut="00:01:07:17" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">Seien Sie vorsichtig.</Text>
      </Subtitle>
      <Subtitle SpotNumber="0021" TimeIn="00:01:07:22" TimeOut="00:01:09:08" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="17.00" Halign="center" Hposition="0.00">- Mr.?</Text>
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">- Bond.</Text>
      </Subtitle>
      <Subtitle SpotNumber="0022" TimeIn="00:01:12:15" TimeOut="00:01:13:20" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">James Bond.</Text>
      </Subtitle>
      <Subtitle SpotNumber="0023" TimeIn="00:01:14:12" TimeOut="00:01:16:12" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">Was wissen Sie über Angst?</Text>
      </Subtitle>
      <Subtitle SpotNumber="0024" TimeIn="00:01:17:07" TimeOut="00:01:18:07" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">Alles.</Text>
      </Subtitle>
      <Subtitle SpotNumber="0025" TimeIn="00:01:18:10" TimeOut="00:01:19:13" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">So nicht.</Text>
      </Subtitle>
      <Subtitle SpotNumber="0026" TimeIn="00:01:20:07" TimeOut="00:01:21:10" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">Nicht wie er.</Text>
      </Subtitle>
      <Subtitle SpotNumber="0027" TimeIn="00:01:23:05" TimeOut="00:01:25:17" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="17.00" Halign="center" Hposition="0.00">Sehen Sie sich an,</Text>
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">wie Sie Spione jagen.</Text>
      </Subtitle>
      <Subtitle SpotNumber="0028" TimeIn="00:01:27:20" TimeOut="00:01:29:22" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">England, das Empire...</Text>
      </Subtitle>
      <Subtitle SpotNumber="0029" TimeIn="00:01:31:22" TimeOut="00:01:33:05" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">MI6...</Text>
      </Subtitle>
      <Subtitle SpotNumber="0030" TimeIn="00:01:35:15" TimeOut="00:01:37:05" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">So altmodisch.</Text>
      </Subtitle>
      <Subtitle SpotNumber="0031" TimeIn="00:01:38:17" TimeOut="00:01:40:22" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="17.00" Halign="center" Hposition="0.00">Sie sollen mich töten,</Text>
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">obwohl sie nicht bereit sind</Text>
      </Subtitle>
      <Subtitle SpotNumber="0032" TimeIn="00:01:41:01" TimeOut="00:01:42:12" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">und wohl sterben.</Text>
      </Subtitle>
      <Subtitle SpotNumber="0033" TimeIn="00:01:42:15" TimeOut="00:01:44:22" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">Mami war sehr böse.</Text>
      </Subtitle>
      <Subtitle SpotNumber="0034" TimeIn="00:01:47:03" TimeOut="00:01:49:15" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="17.00" Halign="center" Hposition="0.00">Es gibt zwei Überlebende.</Text>
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">Das machte sie aus uns.</Text>
      </Subtitle>
      <Subtitle SpotNumber="0035" TimeIn="00:01:50:05" TimeOut="00:01:51:15" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">Jeder braucht ein Hobby.</Text>
      </Subtitle>
      <Subtitle SpotNumber="0036" TimeIn="00:01:51:19" TimeOut="00:01:53:03" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">Was ist Ihres?</Text>
      </Subtitle>
      <Subtitle SpotNumber="0037" TimeIn="00:01:53:07" TimeOut="00:01:54:12" FadeUpTime="00:00:00:02" FadeDownTime="00:00:00:02">
        <Text Valign="bottom" Vposition="10.00" Halign="center" Hposition="0.00">Wiederauferstehung.</Text>
      </Subtitle>
    </Font>
  </SubtitleList>
